    "alias-hq": "6.2.2",
    "cors": "^2.8.5",
    "esbuild": "0.19.3",
    "esbuild-register": "3.5.0",
    "highlight.js": "11.10.0",
    "i": "0.3.7",
    "micromark": "4.0.0",
    "micromark-extension-gfm": "3.0.0",
    "pnpm": "9.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.15.0",
    "route-event": "6.0.0",
